</head>

<body style="display: block;" data-feedly-mini="yes">
    <div>
        <section class="effective-content-area" id="main-content-area">

            <section id="MainArea:SkillsPassport" class="main-area" style="display: block;">
                <section id="Compose:SkillsPassport" class="navigation-area wysiwyg Compose">

                    <div class="cv compose">
                       
                        <dl id="Compose:LearnerInfo.Identification" class="section item opens-modal-form personal" data-rel-doc="cv" data-rel-clazz="PersonalInfoFormView" data-rel-form="Form:LearnerInfo.Identification" data-rel-section="SkillsPassport.LearnerInfo.Identification"
                            data-rel-help-section="Help.SkillsPassport.LearnerInfo.Identification" data-rel-index="" data-validation-field="date currentDate">
                             {{#Identification}}
                            <dt class="label">
                                <span class="title">INFORMAZIONI PERSONALI</span>

                                <div class="clear"></div>

                                <div class="Photo active opens-modal-form nested-modal " data-rel-doc="cv" data-rel-clazz="PhotoFormView" data-rel-form="Form:LearnerInfo.Identification.Photo" data-rel-section="SkillsPassport.LearnerInfo.Identification.Photo" data-rel-help-section="Help.SkillsPassport.LearnerInfo.Identification.Photo" data-rel-index="" rel="tooltip">

                                    <div class="photo-preview-area">
                                        <img style="height:110px;" 
                                            src="data:{{Photo.MimeType}};base64, {{Photo.Data}}" alt="EditorPlaceholder[LearnerInfo.Identification.Photo]">
                                    </div>
                                </div>
                            </dt>

                            <dd class="personal-details value personal">

                                <div class="name value PersonName LearnerInfo.Identification.PersonName">

                                    <span id="LearnerInfo.Identification.PersonName.Surname" class=" Surname">{{PersonName.Surname}}</span>
                                    <span id="LearnerInfo.Identification.PersonName.FirstName" class=" FirstName">{{PersonName.FirstName}}</span>
                                </div>

                                <div class="PersonalBasic">
{{#ContactInfo}}
                                    <div class="value Address" id="LearnerInfo.Identification.ContactInfo.Address">
                                        <span class="Address">
                                            {{#Address.Contact}}{{AddressLine}}, {{PostalCode}} {{Municipality}}, ({{Country.Label}}){{/Address.Contact}}</span>
                                    </div>

                                    <div class="value Telephone ">
                                        {{#each Telephone}}		

                                        <div class="item 0  {{Use.code}}  ">
                                            <span class="Use" style="display:none">{{Use.Label}}</span>
                                            <span class="Contact">{{Contact}}</span>
                                        </div>
			{{/each}}
                                        
                                    </div>

                                    <div class="value Email">
                                        <span class="Use" style="display:none">Email</span>
                                        <span class="Contact"><a href="mailto:{{Email.Contact}}" target="_blank">{{Email.Contact}}</a></span>
                                    </div>

                                    <div class="value Website">
                                        {{#each Website}}
                                        <div class="item {{@index}}">
                                            <span class="Use"></span>
                                            <span class="Contact"><a title="{{Contact}}" href="{{Contact}}" target="_blank">{{Contact}}</a></span>
                                        </div>	
                                        {{/each}}
                                        
                                    </div>

                                    <div class="value InstantMessaging">
                                        {{#each InstantMessaging}}
                                        
                                        <div class="item {{@index}}">
                                            <span class="Use">{{Use.Label}}</span>
                                            <span class="Contact">{{Contact}}</span>
                                        </div>
                                        {{/each}}
                                    </div>
                                    {{/ContactInfo}}
                                </div>

                                <div class="PersonalDetails">
                                    {{#Demographics}}
                                    
                                    <dl class="Gender">
                                        <dt class="label">Sesso</dt>
                                        <dd class="value">
                                            <span class="Gender">{{Gender.Label}}</span>
                                        </dd>
                                    </dl>

                                    <dl class="Birthdate">
                                        <dt class="label">Data di nascita</dt>
                                        <dd class="value">
                                            <span class="Birthdate">
                                                {{#Birthdate}}{{Day}}/{{Month}}/{{Year}}{{/Birthdate}}</span>
                                        </dd>
                                    </dl>

                                    <dl class="Nationality">
                                        <dt class="label">Nazionalità</dt>
                                        <dd class="value">
                                            <span class="Nationality">
                                        {{#each Nationality}}
                                               {{#if @index}}, {{/if}}
                                                {{Label}}
                                        {{/each}}
                                                </span>
                                        </dd>
                                    </dl>
                                    {{/Demographics}}
                                </div>
                            </dd>
                            {{/Identification}}
                        </dl>
                        <dl id="Compose:LearnerInfo.Headline" class="section item opens-modal-form nested-modal clickeffect" data-rel-doc="cv" data-rel-clazz="HeadlineFormView" data-rel-form="Form:LearnerInfo.Headline" data-rel-section="SkillsPassport.LearnerInfo.Headline" data-rel-help-section="Help.SkillsPassport.LearnerInfo.Headline"
                            data-rel-index="">
                            <dt class="label">

                                <span class="title">{{Headline.Type.Label}}</span>
                            </dt>

                            <dd class="value rich-content Description">
                                <div class="value Description Label">{{{Headline.Description.Label}}}</div>
                            </dd>
                        </dl>
                        <div class="work-education-experiences-area">
                            <dl id="Compose:LearnerInfo.WorkExperience" class="section work-education subsection-container Work switch">

                                <dt class="label list section-heading">

                                    <span class="title">ESPERIENZA PROFESSIONALE</span>
                                </dt>
                                <dd class="line"></dd>
                                <ol class="sortable compose-list ui-sortable" id="List:SkillsPassport.LearnerInfo.WorkExperience" data-rel-section="SkillsPassport.LearnerInfo.WorkExperience">
{{#each WorkExperience}}
                                    <li class="compose-list list-item opens-modal-form" id="ListItem:LearnerInfo.WorkExperience[0]" data-index="0" data-rel-doc="cv" data-rel-clazz="WorkExperienceFormView" data-rel-form="Form:WorkExperience[0]" data-rel-section="SkillsPassport.LearnerInfo.WorkExperience[0]"
                                        data-rel-help-section="Help.SkillsPassport.LearnerInfo.WorkExperience" data-rel-index="0" data-validation-field="date period">

                                        <dd class="content-label Period">
                                            <span class="PeriodRange">
                                                <span class="period">{{#Period.From}}{{Day}}/{{Month}}/{{Year}}{{/Period.From}}</span>
                                                {{#if Period.Current}}
                                                <span class="period">–alla data attuale </span>
                                                {{else}}
                                                <span class="period">- {{#Period.To}}{{Day}}/{{Month}}/{{Year}}{{/Period.To}}</span>                                                
                                                {{/if}}
                                            </span>
                                        </dd>

                                        <dd class="value Position">
                                            <span class="Position">{{Position.Label}}</span>
                                        </dd>

                                        <dd class="value Employer">
                                            {{#Employer}}
                                            <span class="Name">
                                                {{#if ContactInfo.Website}}
                                                <a title="{{Name}}" href="{{ContactInfo.Website.Contact}}" target="_blank">{{Name}}</a>
                                                {{else}}
                                                {{Name}}
                                                {{/if}}
                                                </span>
                                            {{#ContactInfo}}
                                            {{#if Address.Contact.AddressLine}}
                                            <div class="Address">
                                                {{else}}
                                            <div class="Address minimal">. 
                                            {{/if}}
                                                <span class="Address">
                                                    {{#if Address.Contact.AddressLine}}
                                                        {{Address.Contact.AddressLine}},
                                                    {{/if}}
                                                    {{#if Address.Contact.PostalCode}}
                                                        {{Address.Contact.PostalCode}},
                                                    {{/if}}
                                                     {{Address.Contact.Municipality}} ({{Address.Contact.Country.Label}})</span>
                                            </div>

                                            {{/ContactInfo}}
                                            {{/Employer}}
                                        </dd>


                                        <dd class="value rich-content Activities">
                                            {{{Activities}}}
                                        </dd>

                                    </li>
{{/each}}
                                </ol>
                            </dl>
                            <dl id="Compose:LearnerInfo.Education" class="section work-education subsection-container Education">
                                <dt class="label list section-heading">

                                    <span class="title">ISTRUZIONE E FORMAZIONE</span>
                                </dt>
                                <dd class="line"></dd>
                                <ol class="sortable compose-list ui-sortable" id="List:SkillsPassport.LearnerInfo.Education" data-rel-section="SkillsPassport.LearnerInfo.Education">

                                    <li class="compose-list list-item opens-modal-form" id="ListItem:LearnerInfo.Education[0]" data-index="0" data-rel-doc="cv" data-rel-clazz="EducationFormView" data-rel-form="Form:LearnerInfo.Education[0]" data-rel-section="SkillsPassport.LearnerInfo.Education[0]"
                                        data-rel-help-section="Help.SkillsPassport.LearnerInfo.Education" data-validation-field="date period" data-rel-index="0">

                                        <dd class="content-label Period ">
                                            <span class="PeriodRange"><span class="period">Set. 05</span><span class="period">–Set. 13</span></span>
                                        </dd>

